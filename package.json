	"bugs": {
		"url": "https://github.com/{{project-org}}/{{project-name}}/issues"
	},
	"homepage": "https://github.com/{{project-org}}/{{project-name}}#readme",
	"keywords": [],
	"dependencies": {
		"ts-commands": "^2.0.0"
	}
}
